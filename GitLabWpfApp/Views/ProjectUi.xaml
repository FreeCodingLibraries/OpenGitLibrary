            <Button Content="Dev" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Center"/>
            <Button Content="UAT" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Center"/>
            <Button Content="UAT-2" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Center" />
            <Button Content="PROD" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Center" />
            <Button Content="PROD-2" HorizontalAlignment="Left" Margin="10,0,0,0" VerticalAlignment="Center" />
        </StackPanel>
        <Button Content="DevEnvs" HorizontalAlignment="Left" VerticalAlignment="Top"
                Margin="10,31,0,0" />
        <Button Content="Tradeweb" HorizontalAlignment="Left" VerticalAlignment="Top"
                Margin="10,95,0,0"  />

    </Grid>
</UserControl>